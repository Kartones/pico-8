pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

-- todo: sprite graphics for ball and paddle


function move_paddle()
-- 0 is ←, 1 is → , 2 is ↑ , 3 is ↓ , 4 is z and 5 is x
 if btn(0) then
  paddle_x = max(paddle_x - paddle_speed, 0)
 elseif btn(1) then
  paddle_x = min(paddle_x + paddle_speed, max_paddle_pos_x)
 end
end

function move_ball()
 ball_x += ball_x_direction
 ball_y += ball_y_direction
end

function bounce_ball()
 if ball_x < ball_size then
  ball_x_direction = -ball_x_direction
  sfx(0)
 end

 if ball_x > max_ball_pos_x then
  ball_x_direction = -ball_x_direction
  sfx(0)
 end

 if ball_y < ball_size then
  ball_y_direction = -ball_y_direction
  sfx(0)
 end
end

function check_gameover_input()
 if btn(5) then
  restart_game()
 end
end

function bounce_ball_paddle()
 if ball_x >= paddle_x and ball_x <= paddle_x + paddle_width then
  if ball_y > paddle_y - ball_size then
   ball_y_direction = -ball_y_direction
   ball_y = paddle_y - ball_size
   score += 5
   sfx(1)
  end
 end
end

function lose_ball()
 if ball_y > 128 then
  if lives > 0 then
   lives -= 1
   ball_y = 24
   sfx(2)
  else
    game_over = true
    ball_x_direction = 0
    ball_y_direction = 0
    ball_y = 140
    ball_x = 64
  end
 end
end

function restart_game()
 -- paddle variables
 paddle_x = 52
 paddle_y = 122
 paddle_width = 24
 paddle_height = 4

 paddle_speed = 4
 max_paddle_pos_x = 128 - paddle_width

 -- ball variables
 ball_x = 64
 ball_y = 64
 ball_size = 3
 ball_x_direction = 5  -- also means horizontal speed
 ball_y_direction = -3 -- also means vertical speed

 max_ball_pos_x = 128 - ball_size

 -- player variables
 score = 0
 lives = 3
 game_over = false
end

function _init()
 restart_game()
end

function _update()
 if game_over == true then
  check_gameover_input()
  return false
 end
 move_paddle()
 bounce_ball()
 bounce_ball_paddle()
 move_ball()
 lose_ball()
end

function _draw()
 -- clear
 rectfill(0, 0, 128, 128, 0)

 if game_over == false then
  -- lives
  for i=1, lives do
   spr(000, 90 + i*8, 4)
  end

  -- score
  print(score, 12, 6, 7)

  -- paddle
  rectfill(paddle_x, paddle_y, paddle_x + paddle_width, paddle_y + paddle_height, 7)

  -- ball
  circfill(ball_x, ball_y, ball_size, 7)
 else
  print("game over", 44, 54, 7)
  print("final score: ", 36, 64, 7)
  print(score, 84, 64, 7)
 end
end


